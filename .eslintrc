{
  "extends": ["airbnb-base", "prettier", "plugin:prettier/recommended"],
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "printWidth": 120,
        "singleQuote": true,
        "trailingComma": "all"
      }
    ]
  }
}
